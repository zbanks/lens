digraph {
    input;
    print_dns [type=print prefix=DNS];
    print_udp [type=print prefix=UDP];
    output [type=output];

    input -> eth_dec;
    eth_dec -> ipv4_dec [ethertype=ipv4];
    ipv4_dec -> udp_dec [proto=udp];
    udp_dec -> dns_dec [port=53];
    dns_dec -> print_dns;
    udp_dec -> print_udp [unmatched=1];
}
